

  <div class="card season-selector">
    <div class="card-header" role="tab" id="headingOne">
      <h5 class="mb-0">
        <a data-toggle="collapse" href="#collapseGdocs" aria-expanded="true" aria-controls="collapseGdocs">
          Sources
        </a>
      </h5>
    </div>
    <div id="collapseGdocs" class="collapse " role="tabpanel" aria-labelledby="headingGdocs" data-parent="#accordion" >
      <div class="card-body">
        <ul>
      </ul>
      </div>
    </div>
  </div>

  <script>
  getsy('http://itswatchseries.to/episode/<%= tvInfo.name.replace(/[^\w\s]/gi, '').replace(/\s+/g, '_').toLowerCase() %>_s<%= seasonNum%>_e<%= episodeNum %>.html', {corsProxy: 'https://cors-anywhere.herokuapp.com/'}).then(myGetsy => {
    let unparsedLinks = myGetsy.getMe('td.deletelinks a')
    let links = []
    for (let i = 0; i < unparsedLinks.length; i++) {
      links.push(unparsedLinks[i].getAttribute('onclick').replace(/if \(confirm\('Delete link /g, '').replace(/'\)\).*$/, ''))
    }
    console.log(links)
    document.querySelector("iframe").src = links[0];

    for (var c in links) {
    var newElement = document.createElement('div');
    newElement.id = links[c]; newElement.className = "car";
    newElement.innerHTML = links[c];
    document.body.appendChild(newElement);
}

  })
  </script>
