                  <ul>
                  <% for (let i = 0; i < openloadLinks.length; i++) { %>
                    <li><button onclick="document.querySelector('iframe').setAttribute('src', '<%= openloadLinks[i] %>')" class="source btn" title="<%= openloadTitles[i] %>"><%= openloadTitles[i] %></button></li>
                  <% } %>
                </ul>
                </div>
              </div>
            </div>
            <% } %>
