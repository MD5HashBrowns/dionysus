            <% if (gdocsLinks.length > 0) { %>
            <div class="card season-selector">
              <div class="card-header" role="tab" id="headingOne">
                <h5 class="mb-0">
                  <a data-toggle="collapse" href="#collapseGdocs" aria-expanded="true" aria-controls="collapseGdocs">
                    Google Drive
                  </a>
                </h5>
              </div>
              <div id="collapseGdocs" class="collapse " role="tabpanel" aria-labelledby="headingGdocs" data-parent="#accordion" >
                <div class="card-body">
                  <ul>
                  <% for (let i = 0; i < gdocsLinks.length; i++) { %>
                    <li><button onclick="document.querySelector('iframe').setAttribute('src', '<%= gdocsLinks[i] %>')" class="source btn" title="<%= gdocsTitles[i] %>"><%= gdocsTitles[i] %></button></li>
                  <% } %>
                </ul>
                </div>
              </div>
            </div>
            <% } %>

            <% if (thevideoLinks.length > 0) { %>
            <div class="card season-selector">
              <div class="card-header" role="tab" id="headingOne">
                <h5 class="mb-0">
                  <a data-toggle="collapse" href="#collapseThevideo" aria-expanded="true" aria-controls="collapseThevideo">
                    TheVideo.me
                  </a>
                </h5>
              </div>
              <div id="collapseThevideo" class="collapse " role="tabpanel" aria-labelledby="headingGdocs" data-parent="#accordion" >
                <div class="card-body">
                  <ul>
                  <% for (let i = 0; i < thevideoLinks.length; i++) { %>
                    <li><button onclick="document.querySelector('iframe').setAttribute('src', '/watch-thevideo/<%= thevideoLinks[i].substring(20)  %>')" class="source btn" title="<%= thevideoTitles[i] %>"><%= thevideoTitles[i] %></button></li>
                  <% } %>
                </ul>
                </div>
              </div>
            </div>
            <% } %>

            <% if (openloadLinks.length > 0) { %>
            <div class="card season-selector">
              <div class="card-header" role="tab" id="headingOne">
                <h5 class="mb-0">
                  <a data-toggle="collapse" href="#collapseOpenload" aria-expanded="true" aria-controls="collapseOpenload">
                    Openload.co
                  </a>
                </h5>
              </div>
              <div id="collapseOpenload" class="collapse " role="tabpanel" aria-labelledby="headingOpenload" data-parent="#accordion" >
